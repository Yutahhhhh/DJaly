{
  "productName": "Djaly",
  "version": "0.1.1",
  "identifier": "com.djaly.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "bin/djaly-server"
    ],
    "resources": [],
    "macOS": {
      "entitlements": "Entitlements.plist"
    }
  },
  "app": {
    "windows": [
      {
        "title": "Djaly",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "withGlobalTauri": true,
    "security": {
      "csp": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; connect-src 'self' ipc: http://127.0.0.1:48123 ws://127.0.0.1:48123 http://localhost:48123 ws://localhost:48123 http://127.0.0.1:8001 ws://127.0.0.1:8001 http://localhost:8001 ws://localhost:8001; img-src 'self' asset: https: data: blob:; media-src 'self' asset: https: data: blob: http://127.0.0.1:48123 http://localhost:48123 http://127.0.0.1:8001 http://localhost:8001;"
    }
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "updater": {
      "endpoints": [
        "https://github.com/Yutahhhhh/DJaly/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDFGQjU1RjVEQzA3M0NFNzgKUldSNHpuUEFYVisxSDFaV2YzNW1Ha1RQdnhhby9IRlR0MXdvbndSWElKa2RhaDhwdnBhU2NKdUcK"
    }
  }
}